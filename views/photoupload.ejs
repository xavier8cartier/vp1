    <h2>Galerii üles laadimine</h2>

    <form method="POST" enctype="multipart/form-data">
      <label for="photoInput">Vali soovitud pildifail: </label>
      <input id="photoInput" name="photoInput" type="file"

      <label for="altInput">Pildi alternatiivtekst: </label>
    
      <input
        type="text"
        id="altInput"
        name="altInput"
        placeholder="Pildi alternatiivtekst ..."
      />
      <br />
      <br>
      <input type="radio" id="privacyInput1" name="privacyInput" value ="1" checked>
      <label for="privacyInput1">Privaatne (ainult ise näed)</label>
      <br>
      <input type="radio" id="privacyInput2" name="privacyInput" value ="2">
      <label for="privacyInput2">Sisseloginud kasutajatele</label>
      <br>
      <input type="radio" id="privacyInput3" name="privacyInput" value ="3">
      <label for="privacyInput3">Avalik</label>
      <br>
      <input
        type="submit"
        id="photoSubmit"
        name="photoSubmit"
        value="Lae üles"
      />
    </form>
  </body>
